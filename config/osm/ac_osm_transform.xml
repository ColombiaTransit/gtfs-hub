<translations>
 <translation>
  <name>Undo all constructions</name>
  <description>This transformation reverts all highway constructions.</description>
  <match type="way">
    <tag k="highway" v="construction"/>
    <tag k="construction" match_id="c" v=".*"/>
    <tag k="access" v="yes"/>
  </match>
  <output>
   <copy-unmatched />
   <tag from_match="c" k="highway" v="{0}"/>
  </output>
 </translation>
</translations>
